<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gimnasio_unne">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE  " />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Reservar"
            android:parentActivityName=".view.AlumnoActivity"></activity>
        <activity
            android:name=".EditarProfesor"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".view.ActivityPersonal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.AlumnoActivity"
            android:label="@string/title_activity_alumno"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AltaPersona"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".AltaGrupo"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".DetallesPersonas"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".EditarGrupos"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".DetallesGrupo"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".view.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AbmProfesor" />
        <activity android:name=".view.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>